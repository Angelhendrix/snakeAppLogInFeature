<!DOCTYPE html>
<html>
<head>
	<title>SNAKE</title>
<!-- 	<link rel="stylesheet" href="/css/bootstrap.css" />
--><link rel="stylesheet" href="/css/style.css" />
</head>
<body>
	<div class="container">
      <div class="header clearfix">
      <div>
      </div>
        <nav>
          <div class="nav nav-pills pull-right">
            {{#if user}}
              <span role="presentation"><a href="/">SNAKE Dashboard</a></span>
              <span role="presentation"><a href="/users/logout">Logout</a></span>
            {{else}}
              <span role="presentation"><a href="/users/login">Login</a></span>
              <span role="presentation"><a href="/users/register">Register</a></span>
            {{/if}}
          </div>
        </nav>
        <h3 class="text-muted">SNAKE</h3>
      </div>


      <div class="row">
        <div class="col-lg-12">
          {{#if success_msg}}
            <div class="alert alert-success">{{success_msg}}</div>
          {{/if}}

          {{#if error_msg}}
            <div class="alert alert-danger">{{error_msg}}</div>
          {{/if}}

          {{#if error}}
            <div class="alert alert-danger">{{error}}</div>
          {{/if}}
          
          {{{body}}}
        </div>

        
      </div>

    </div> <!-- /container -->
</body>
</html>
